    <string name="shell_netcfg">netcfg</string>
    <string name="shell_ifconfig_dash_a">ifconfig -a</string>
    <string name="shell_ip_addr">ip addr</string>
    <string name="shell_ip_neigh">ip neigh</string>
    <string name="shell_ip_inet6_addr">ip -f inet6 addr</string>
    <string name="shell_ip_route_show">ip route show</string>
    <string name="shell_ip_inet6_route_show">ip -f inet6 route show</string>
    <string name="shell_route_n">route -n</string>
    <string name="shell_route_inet6">route -A inet6 -n</string>
    <string name="shell_df_ah">df -ah</string>
    <string name="shell_ps">ps</string>
    <string name="shell_netstat_r">netstat -rpnW</string>
    <string name="shell_netstat_a">netstat -apnW</string>
    <string name="shell_uname_a">uname -a</string>
    <string name="shell_lsmod">lsmod</string>

    <!-- Proc  -->
    <string name="shell_cat_proc_version">cat /proc/version</string>
    <string name="shell_cat_proc_devices">cat /proc/devices</string>
    <string name="shell_cat_proc_mounts">cat /proc/mounts</string>
    <string name="shell_cat_proc_meminfo">cat /proc/meminfo</string>
    <string name="shell_cat_proc_net_route">cat /proc/net/route</string>
    <string name="shell_cat_proc_net_if_inet6">cat /proc/net/if_inet6</string>
    <string name="shell_cat_proc_net_ipv6_route">cat /proc/net/ipv6_route</string>
    <string name="shell_cat_proc_net_arp">cat /proc/net/arp</string>
    <string name="shell_cat_proc_net_wireless">cat /proc/net/wireless</string>

    <!-- Getprop  -->
    <string name="shell_getprop_product_version">getprop ro.product.version</string>
    <string name="shell_getprop_build_fingerprint">getprop ro.build.fingerprint</string>
    <string name="shell_getprop_gsm_baseband">getprop gsm.version.baseband</string>
    <string name="shell_getprop_vm_heapsize">getprop dalvik.vm.heapsize</string>
    <string name="shell_getprop_lcd_density">getprop ro.sf.lcd_density</string>
    <string name="shell_getprop_vm_execution_mode">getprop dalvik.vm.execution-mode</string>
