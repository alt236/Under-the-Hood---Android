<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- IpConfig -->
    <string-array name="commands_ip_v4">
        <item>ip addr</item>
        <item>ip neigh</item>
        <item>ip route show</item>
        <item>route -n</item>
    </string-array>

    <string-array name="commands_ip_v6">
        <item>ip -f inet6 addr</item>
        <item>ip -f inet6 neigh</item>
        <item>ip -f inet6 route show</item>
        <item>route -A inet6 -n</item>
    </string-array>

    <string-array name="commands_ip_config">
        <item>ifconfig -a</item>
        <item>netcfg</item>
    </string-array>

    <!-- ps -->
    <string-array name="commands_ps">
        <item>ps</item>
    </string-array>

    <!-- Netstat -->
    <string-array name="commands_netstat">
        <item>netstat -apnW</item>
        <item>netstat -rpnW</item>
    </string-array>

    <!-- Other  -->
    <string-array name="commands_other">
        <item>df -ah</item>
        <item>uname -a</item>
        <item>lsmod</item>
    </string-array>

    <!-- Proc  -->
    <string-array name="commands_proc_general">
        <item>cat /proc/meminfo</item>
        <item>cat /proc/uptime</item>
        <item>cat /proc/version</item>
    </string-array>

    <string-array name="commands_proc_hardware">
        <item>cat /proc/cpuinfo</item>
        <item>cat /proc/devices</item>
        <item>cat /proc/mounts</item>
    </string-array>

    <string-array name="commands_proc_network">
        <item>cat /proc/net/arp</item>
        <item>cat /proc/net/if_inet6</item>
        <item>cat /proc/net/ipv6_route</item>
        <item>cat /proc/net/route</item>
        <item>cat /proc/net/wireless</item>
    </string-array>

    <!-- Getprop  -->
    <string-array name="commands_geprop">
        <item>getprop</item>
    </string-array>

    <!-- Hardware -->
    <string-array name="commands_hardware">
        <item>lshal</item>
        <item>lspci</item>
        <item>lsusb</item>
    </string-array>
</resources>